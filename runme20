
cargo build --profile release \
    --features=runtime-benchmarks \
    --manifest-path=node/Cargo.toml || exit

 
./target/release/nodle-parachain benchmark pallet     --chain=dev     --steps=50     --repeat=20     --pallet=pallet_scheduler     --extrinsic='*'     --wasm-execution=compiled     --template=./.maintain/external_pallet_weights.hbs     --output=runtimes/eden/src/weights

